{
  "team_presets": {
    "lean": {
      "description": "Minimal team for small projects or MVP - 2-3 agents covering core QA functions",
      "agent_count": 3,
      "agents": ["qa-manager", "qa-executor", "qa-analyst"],
      "workflow_mode": "consolidated"
    },
    "standard": {
      "description": "Standard team for most projects - 6 agents matching current architecture",
      "agent_count": 6,
      "agents": ["qa-manager", "senior-qa", "junior-qa", "qa-analyst", "security-compliance", "performance"],
      "workflow_mode": "specialized"
    },
    "large": {
      "description": "Large team for enterprise projects - 9+ specialized agents",
      "agent_count": 9,
      "agents": ["qa-manager", "senior-test-architect", "test-execution-engine", "quality-intelligence-analyst", "security-privacy-guardian", "performance-guardian", "ux-researcher", "localization-specialist", "devops-integration-specialist"],
      "workflow_mode": "full"
    }
  },
  "agent_roles": {
    "qa-manager": {
      "name": "QA Manager",
      "alias": ["orchestrator", "test-lead"],
      "focus": "Orchestration, test planning, delegation, fuzzy verification",
      "tools": ["TestPlanDecompositionTool", "FuzzyVerificationTool"],
      "complexity": "high",
      "required_for": ["lean", "standard", "large"]
    },
    "senior-qa": {
      "name": "Senior QA Engineer",
      "alias": ["senior-test-architect", "test-architect"],
      "focus": "Self-healing UI, model-based testing, edge case analysis, complex scenarios",
      "tools": ["SelfHealingTool", "ModelBasedTestingTool", "EdgeCaseAnalysisTool"],
      "complexity": "high",
      "required_for": ["standard", "large"]
    },
    "junior-qa": {
      "name": "Junior QA Worker",
      "alias": ["test-executor", "qa-automation-engineer"],
      "focus": "Regression execution, synthetic data generation, test optimization",
      "tools": ["RegressionTestingTool", "SyntheticDataGeneratorTool", "TestExecutionOptimizerTool", "VisualRegressionTool"],
      "complexity": "medium",
      "required_for": ["standard"]
    },
    "qa-executor": {
      "name": "QA Executor",
      "alias": ["test-automation-specialist"],
      "focus": "Consolidated test execution, data generation, optimization, UX, localization (combined role for lean teams)",
      "tools": ["RegressionTestingTool", "SyntheticDataGeneratorTool", "TestExecutionOptimizerTool", "VisualRegressionTool", "FlakyTestDetectionTool", "UXUsabilityTestingTool", "LocalizationTestingTool"],
      "complexity": "medium",
      "required_for": ["lean"]
    },
    "test-execution-engine": {
      "name": "Test Execution Engine",
      "alias": ["bulk-test-runner", "regression-specialist"],
      "focus": "Bulk test execution, parallel runs, regression optimization",
      "tools": ["RegressionTestingTool", "TestExecutionOptimizerTool", "FlakyTestDetectionTool"],
      "complexity": "medium",
      "required_for": ["large"]
    },
    "qa-analyst": {
      "name": "QA Analyst",
      "alias": ["quality-analyst", "test-analyst"],
      "focus": "Reporting, data aggregation, traceability, coverage analysis",
      "tools": ["DataOrganizationReportingTool", "SecurityAssessmentTool", "PerformanceProfilingTool", "TestTraceabilityTool"],
      "complexity": "medium",
      "required_for": ["lean", "standard", "large"]
    },
    "quality-intelligence-analyst": {
      "name": "Quality Intelligence Analyst",
      "alias": ["analytics-specialist", "insights-analyst"],
      "focus": "Advanced analytics, predictive quality, trend analysis, ML-driven insights",
      "tools": ["DataOrganizationReportingTool", "TestTraceabilityTool", "PredictiveAnalyticsTool"],
      "complexity": "high",
      "required_for": ["large"]
    },
    "security-compliance": {
      "name": "Security & Compliance Agent",
      "alias": ["security-guardian", "compliance-officer"],
      "focus": "OWASP testing, GDPR/PCI DSS compliance, vulnerability scanning",
      "tools": ["ComprehensiveSecurityAssessmentTool", "GDPRComplianceTool", "PCIDSSComplianceTool"],
      "complexity": "high",
      "required_for": ["standard"]
    },
    "security-privacy-guardian": {
      "name": "Security & Privacy Guardian",
      "alias": ["security-specialist", "privacy-officer"],
      "focus": "Security testing, penetration testing, privacy protection, compliance validation",
      "tools": ["ComprehensiveSecurityAssessmentTool", "GDPRComplianceTool", "PCIDSSComplianceTool", "PrivacyProtectionTool"],
      "complexity": "high",
      "required_for": ["large"]
    },
    "performance": {
      "name": "Performance & Resilience Agent",
      "alias": ["performance-specialist", "sre"],
      "focus": "Performance monitoring, load testing, resilience validation",
      "tools": ["PerformanceMonitoringTool", "LoadTestingTool", "ResilienceValidationTool"],
      "complexity": "high",
      "required_for": ["standard"]
    },
    "performance-guardian": {
      "name": "Performance Guardian",
      "alias": ["performance-engineer", "load-testing-specialist"],
      "focus": "Advanced performance profiling, load testing, stress testing, bottleneck analysis",
      "tools": ["PerformanceMonitoringTool", "LoadTestingTool", "ResilienceValidationTool", "AdvancedProfilingTool"],
      "complexity": "high",
      "required_for": ["large"]
    },
    "ux-researcher": {
      "name": "UX Researcher",
      "alias": ["usability-specialist", "ux-analyst"],
      "focus": "User experience testing, session analysis, heatmaps, A/B testing",
      "tools": ["UXUsabilityTestingTool"],
      "complexity": "medium",
      "required_for": ["large"]
    },
    "localization-specialist": {
      "name": "Localization Specialist",
      "alias": ["i18n-specialist", "globalization-tester"],
      "focus": "i18n testing, localization validation, RTL support, cultural formatting",
      "tools": ["LocalizationTestingTool"],
      "complexity": "medium",
      "required_for": ["large"]
    },
    "devops-integration-specialist": {
      "name": "DevOps Integration Specialist",
      "alias": ["ci-cd-specialist", "pipeline-engineer"],
      "focus": "CI/CD integration, pipeline automation, artifact management, PR checks",
      "tools": ["CIPipelineIntegrationTool", "ArtifactManagementTool"],
      "complexity": "medium",
      "required_for": ["large"]
    }
  },
  "workflows": {
    "consolidated": {
      "description": "Lean team workflow - agents handle multiple responsibilities",
      "delegation_rules": {
        "complexity_threshold": "high",
        "route_to": ["qa-manager"]
      },
      "agent_capabilities": {
        "qa-manager": ["planning", "verification", "delegation"],
        "qa-executor": ["execution", "data", "ux", "localization", "security", "performance"],
        "qa-analyst": ["reporting", "traceability", "security", "performance"]
      }
    },
    "specialized": {
      "description": "Standard workflow - clear role separation",
      "delegation_rules": {
        "complexity_threshold": "medium",
        "route_to": {
          "security": "security-compliance",
          "performance": "performance",
          "complex": "senior-qa",
          "execution": "junior-qa",
          "reporting": "qa-analyst"
        }
      },
      "agent_capabilities": {
        "qa-manager": ["planning", "verification", "delegation"],
        "senior-qa": ["complex-scenarios", "self-healing", "model-testing", "edge-cases"],
        "junior-qa": ["execution", "data", "optimization", "visual"],
        "qa-analyst": ["reporting", "traceability"],
        "security-compliance": ["security", "compliance"],
        "performance": ["monitoring", "load", "resilience"]
      }
    },
    "full": {
      "description": "Enterprise workflow - maximum specialization",
      "delegation_rules": {
        "complexity_threshold": "low",
        "route_to": {
          "security": "security-privacy-guardian",
          "performance": "performance-guardian",
          "ux": "ux-researcher",
          "localization": "localization-specialist",
          "execution": "test-execution-engine",
          "analytics": "quality-intelligence-analyst",
          "ci-cd": "devops-integration-specialist",
          "complex": "senior-test-architect"
        }
      },
      "agent_capabilities": {
        "qa-manager": ["planning", "verification", "delegation", "oversight"],
        "senior-test-architect": ["complex-scenarios", "architecture", "model-testing"],
        "test-execution-engine": ["bulk-execution", "regression", "optimization"],
        "quality-intelligence-analyst": ["analytics", "predictive", "trends"],
        "security-privacy-guardian": ["security", "privacy", "compliance"],
        "performance-guardian": ["profiling", "load", "stress", "resilience"],
        "ux-researcher": ["usability", "sessions", "heatmaps", "ab-testing"],
        "localization-specialist": ["i18n", "rtl", "cultural"],
        "devops-integration-specialist": ["ci-cd", "pipelines", "artifacts"]
      }
    }
  },
  "team_size_mapping": {
    "small_project": "lean",
    "medium_project": "standard",
    "large_project": "large",
    "enterprise": "large"
  },
  "complexity_routing": {
    "trivial": {
      "route_to": "junior-qa",
      "description": "Simple, repetitive tests - fast execution"
    },
    "simple": {
      "route_to": "junior-qa",
      "description": "Straightforward test cases with clear pass/fail"
    },
    "moderate": {
      "route_to": "junior-qa",
      "description": "Standard test scenarios with some complexity"
    },
    "complex": {
      "route_to": "senior-qa",
      "description": "Complex scenarios requiring expert judgment"
    },
    "critical": {
      "route_to": "senior-qa",
      "description": "High-stakes tests requiring senior expertise"
    }
  },
  "default_team_size": "standard",
  "supports_dynamic_scaling": true
}
